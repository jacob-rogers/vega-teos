{
  "name": "vega-2-teos",
  "version": "0.0.1",
  "description": "Проект VEGA 2.0 TEOS",
  "author": "lanit-tercom",
  "license": "UNLICENSED",
  "engines": {
    "node": "^12.16.2 || ^16",
    "yarn": "^1.22.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "./node_modules/@gpn-prototypes/frontend-configs/git/commitizen.js"
    }
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --progress --config ./webpack.forShellConfig.js",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --mode development",
    "dev:shell": "cross-env NODE_ENV=development webpack-dev-server --mode development --port 9008 --config ./webpack.forShellConfig.js",
    "gen:codegen": "graphql-codegen --config codegen.yml",
    "server": "cross-env NODE_ENV=development webpack-dev-server --host 0.0.0.0 --disable-host-check --open --mode development",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "format": "prettier --write \"{src,types,scripts}/**/*.{json,{j,t}s{,x},css,}\"",
    "coverage": "jest --all --coverage",
    "lint": "npm-run-all lint:*",
    "lint-fix": "npm-run-all format lint-fix:*",
    "lint:ts": "eslint \"{src,types}/**/*.ts{,x}\"",
    "lint-fix:ts": "eslint \"{src,types}/**/*.ts{,x}\" --fix",
    "lint:css": "stylelint \"src/**/*.css\"",
    "lint-fix:css": "stylelint \"src/**/*.css\" --fix"
  },
  "dependencies": {
    "@apollo/client": "^3.3.11",
    "@consta/uikit": "^3.18.3",
    "@gpn-prototypes/vega-ui": "^4.0.11",
    "array-to-tree": "^3.3.2",
    "graphql": "^16.3.0",
    "lodash": "^4.17.20",
    "lodash.defaultto": "^4.14.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-redux": "^7.2.0",
    "react-router-dom": "^6.3.0",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-observable": "^2.0.0",
    "reselect": "^4.1.5",
    "single-spa-react": "^4.6.1",
    "systemjs-webpack-interop": "^2.1.2",
    "typescript-fsa": "^3.0.0",
    "typescript-fsa-reducers": "^1.2.2",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
    "@gpn-prototypes/frontend-configs": "^1.4.10",
    "@graphql-codegen/cli": "2.6.2",
    "@graphql-codegen/introspection": "2.1.1",
    "@graphql-codegen/typescript": "^2.4.8",
    "@graphql-codegen/typescript-operations": "2.3.5",
    "@graphql-codegen/typescript-react-apollo": "3.2.11",
    "@svgr/webpack": "5.5.0",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^13.0.0",
    "@types/jest": "^27.4.1",
    "@types/lodash": "^4.14.165",
    "@types/node": "^17.0.23",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/react-redux": "^7.1.9",
    "@types/react-router-dom": "^5.1.5",
    "@types/redux": "^3.6.0",
    "@types/redux-logger": "^3.0.8",
    "@types/redux-mock-store": "^1.0.2",
    "@types/single-spa-react": "^4.0.0",
    "commitizen": "^4.2.1",
    "cross-env": "^7.0.2",
    "cz-customizable": "^6.3.0",
    "dotenv": "^16.0.0",
    "eslint-plugin-react-hooks": "^4.4.0",
    "file-loader": "^6.1.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "jest-junit": "^13.1.0",
    "lint-staged": "^12.3.7",
    "npm-run-all": "^4.1.5",
    "path": "^0.12.7",
    "postcss": "^8.1.1",
    "postcss-loader": "^4.0.4",
    "stylelint": "^14.8.2",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-prettier": "^2.0.0",
    "url-loader": "^4.1.1",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-config-single-spa-react": "^1.12.0",
    "webpack-config-single-spa-react-ts": "^1.12.0",
    "webpack-config-single-spa-ts": "^1.12.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-import-map-plugin": "^1.0.0-alpha0",
    "webpack-merge": "^4.2.2"
